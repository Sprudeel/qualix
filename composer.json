{
  "name": "gloggi/qualix",
  "type": "project",
  "description": "Qualix. Webapp for tracking and managing observations relevant to qualifications in Swiss J+S courses of the scouting movement.",
  "keywords": [
    "qualix",
    "scouting",
    "pfadi"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2.5",
    "ext-json": "*",
    "calebporzio/parental": "^0.10.0",
    "doctrine/dbal": "^2.9",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "laravel/framework": "^7.0",
    "laravel/socialite": "^4.2",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "phpoffice/phpspreadsheet": "^1.10",
    "sentry/sentry-laravel": "^1.7",
    "troelskn/laravel-fillable-relations": "master"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.0",
    "noeldemartin/laravel-cypress": "^0.1.1",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5",
    "spatie/laravel-db-snapshots": "^1.6.2",
    "symfony/dom-crawler": "^5.1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/Http/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
